 \"nav-menu active\" : \"nav-menu\"}>\r\n          <li className=\"nav-item\">\r\n            <Link to=\"/\" className=\"nav-links\" onClick={handleLogout}>\r\n             Log out\r\n            </Link>\r\n          </li>\r\n          <li className=\"nav-item\" onMouseEnter={onMouseEnter}\r\n              onMouseLeave={onMouseLeave}>\r\n            <Link\r\n              \r\n              className=\"nav-links\"\r\n              onClick={closeMobileMenu}\r\n              \r\n            >\r\n              Services <i className=\"fas fa-caret-down\" />\r\n            </Link>\r\n            {dropdown && <Dropdown />}\r\n          </li>\r\n\r\n          <li className=\"nav-item\">\r\n            <Link to=\"/\" className=\"nav-links\" onClick={closeMobileMenu}>\r\n              {\" \"}\r\n              Contact Us\r\n            </Link>\r\n          </li>\r\n          <li className=\"nav-item\">\r\n            <Link to=\"/\" className=\"nav-links\" onClick={closeMobileMenu}>\r\n              {\" \"}\r\n              Feedback{\" \"}\r\n            </Link>\r\n          </li>\r\n\r\n          <li className=\"nav-item\">\r\n\r\n            { currentUser ?    currentUser.email : <Button /> }\r\n            \r\n          </li>\r\n        </ul>\r\n        <li className=\"nav-item\">\r\n           \r\n           <Link to=\"/\" >\r\n           <AiOutlineShoppingCart size=\"30px\" color=\"#fff\" />\r\n             </Link>  \r\n        </li>\r\n\r\n        <li className=\"nav-item\">\r\n        \r\n          <p className=\"nav-p\" onClick={toggle}>\r\n            Menu\r\n          </p>\r\n        </li>\r\n      </nav>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Navbar;\r\n","G:\\study materials\\SWE 3.2\\ReactProjects\\LabTasks\\restaurant-website\\src\\components\\Navbar\\UserNavbar\\Dropdown.js",[],"G:\\study materials\\SWE 3.2\\ReactProjects\\LabTasks\\restaurant-website\\src\\components\\Navbar\\UserNavbar\\MenuItem.js",[],"G:\\study materials\\SWE 3.2\\ReactProjects\\LabTa